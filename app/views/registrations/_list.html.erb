

<div id="filterrific_results" class="row">

  <div class="col-lg-2 col-md-0"></div>
  <div class="col-lg-8 col-md-12">
    <table class="table table-striped table-inverse">
      <tr>
        <h4 colspan="6" style="text-align:center">Registrations</h4>
      </tr>
      <tr>
        <h5 colspan="6" style="text-align:center"><%= page_entries_info registrations%></h5>
      </tr>
      <tr>
        <th><%= filterrific_sorting_link(@filterrific, :Name) %></th>
        <th><%= filterrific_sorting_link(@filterrific, :Event) %></th>
        <th>Contact_no</th>
        <th>Email</th>
        <th><%= filterrific_sorting_link(@filterrific, :College) %></th>
        <th><%= filterrific_sorting_link(@filterrific, :created_at) %></th>
      </tr>

      <% registrations.each do |registration| %>
        <tr>
          <td><%= registration.Name %></td>
          <td><%= registration.Event %></td>
          <td><%= registration.Contact_no %></td>
          <td><%= registration.Email %></td>
          <td><%= registration.College %></td>
          <td><%= registration.created_at %></td>
        </tr>
      <% end %>

    </table>
    <tr>
      <%= will_paginate registrations %></td>
    </tr>
  </div>
  <div class="col-lg-2 col-md-0"></div>
</div>
