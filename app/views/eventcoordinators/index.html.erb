<p id="notice"><%= notice %></p>

<h1>Eventcoordinators</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Name</th>
      <th>Name</th>
      <th>Contact no</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eventcoordinators.each do |eventcoordinator| %>
      <tr>
        <td><%= eventcoordinator.Event %></td>
        <td><%= eventcoordinator.Name %></td>
        <td><%= eventcoordinator.Contact_no %></td>
        <td><%= eventcoordinator.Email %></td>
        <td><%= link_to 'Show', eventcoordinator %></td>
        <td><%= link_to 'Edit', edit_eventcoordinator_path(eventcoordinator) %></td>
        <td><%= link_to 'Destroy', eventcoordinator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Eventcoordinator', new_eventcoordinator_path %>
