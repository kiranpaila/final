<%= raw(@html) %>


<h6><%=  session[:em] %></h6>

<form action="adduser" method="POST">

<%= label_tag :name ,"Name" %>
<%= text_field_tag :name %>



<%= label_tag :pass ,"Password" %>
<%= password_field_tag :pass %>



<%= label_tag :cpass ,"Confirm Password" %>
<%= password_field_tag :cpass %>

<%= hidden_field_tag :authenticity_token, form_authenticity_token %>

<%= submit_tag %>

</form>